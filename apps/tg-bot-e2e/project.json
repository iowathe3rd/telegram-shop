{
  "name": "tg-bot-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/tg-bot-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/tg-bot-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "tg-bot:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "tg-bot:serve:production"
        },
        "ci": {
          "devServerTarget": "tg-bot:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": [],
  "implicitDependencies": ["tg-bot"]
}
